Document(
    Prefix(ex <http://example.com/>)
    Prefix(pred <http://www.w3.org/2007/rif-builtin-predicate#>)
    Group (
      Forall ?item (
        If And (
          ?item # ex:item
          Not (Exists ?x ?list ( And (
            ?x[ex:target-list -> ?list]
            External(pred:list-contains(?list ?item))
          )))
        ) Then ex:ok()
      )
    )
)
