@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rif: <http://www.w3.org/2007/rif#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

[] a rif:Document ;
    rif:directives ( [ a rif:Import ;
                rif:location "http://www.w3.org/2005/rules/test/repository/tc/RDF_Combination_Blank_Node/RDF_Combination_Blank_Node-import001" ;
                rif:profile "http://www.w3.org/ns/entailment/RDF" ] ) ;
    rif:payload [ a rif:Group ;
            rif:sentences ( [ a rif:Forall ;
                        rif:formula [ a rif:Implies ;
                                rif:if [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "x" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://example.org/example#hasName"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:Var ;
                                                            rif:varname "y" ] ] ) ] ;
                                rif:then [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "x" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://www.w3.org/1999/02/22-rdf-syntax-ns#type"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:Const ;
                                                            rif:constIRI "http://example.org/example#named"^^xsd:anyURI ] ] ) ] ] ;
                        rif:vars ( [ a rif:Var ;
                                    rif:varname "x" ] [ a rif:Var ;
                                    rif:varname "y" ] ) ] ) ] .

