                                            rif:left [ a rif:External ;
                                                    rif:content [ a rif:Expr ;
                                                            rif:args ( [ a rif:Const ;
                                                                        rif:value "Hello World!@en"^^xsd:string ] ) ;
                                                            rif:op [ a rif:Const ;
