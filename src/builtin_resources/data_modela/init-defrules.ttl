@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rif: <http://www.w3.org/2007/rif#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/add-action-to-defrule> a rif:Forall ;
    rif:formula [ a rif:Implies ;
            rif:if [ a rif:And ;
                    rif:formulas ( [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "rule" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://example.com/uses-action-from"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Var ;
                                                    rif:varname "formula" ] ] ) ] [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "formula" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://example.com/as-action"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Var ;
                                                    rif:varname "actionlist" ] ] ) ] ) ] ;
            rif:then [ a rif:Do ;
                    rif:actions ( [ a rif:Assert ;
                                rif:target [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "rule" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://clips.script/action"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:Var ;
                                                            rif:varname "actionlist" ] ] ) ] ] ) ] ] ;
    rif:vars ( [ a rif:Var ;
                rif:varname "rule" ] [ a rif:Var ;
                rif:varname "formula" ] [ a rif:Var ;
                rif:varname "actionlist" ] ) .

<http://example.com/add-conditional-to-defrule> a rif:Forall ;
    rif:formula [ a rif:Implies ;
            rif:if [ a rif:And ;
                    rif:formulas ( [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "clipsrule" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://example.com/uses-as-conditional"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Var ;
                                                    rif:varname "rifrule" ] ] ) ] [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "rifrule" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://example.com/as-conditional"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Var ;
                                                    rif:varname "conditionlist" ] ] ) ] ) ] ;
            rif:then [ a rif:Do ;
                    rif:actions ( [ a rif:Assert ;
                                rif:target [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "clipsrule" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://clips.script/conditional-element"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:Var ;
                                                            rif:varname "conditionlist" ] ] ) ] ] ) ] ] ;
    rif:vars ( [ a rif:Var ;
                rif:varname "clipsrule" ] [ a rif:Var ;
                rif:varname "rifrule" ] [ a rif:Var ;
                rif:varname "conditionlist" ] ) .

<http://example.com/init-defrule-per-forall> a rif:Forall ;
    rif:formula [ a rif:Implies ;
            rif:if [ a rif:And ;
                    rif:formulas ( [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "rule" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://www.w3.org/1999/02/22-rdf-syntax-ns#type"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Const ;
                                                    rif:constIRI "http://www.w3.org/2007/rif#Forall"^^xsd:anyURI ] ] ) ] [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "rule" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://www.w3.org/2007/rif#formula"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Var ;
                                                    rif:varname "formula" ] ] ) ] [ a rif:INeg ;
                                rif:formula [ a rif:Exists ;
                                        rif:formula [ a rif:Frame ;
                                                rif:object [ a rif:Var ;
                                                        rif:varname "outerrule" ] ;
                                                rif:slots ( [ a rif:Slot ;
                                                            rif:slotkey [ a rif:Const ;
                                                                    rif:constIRI "http://www.w3.org/2007/rif#formula"^^xsd:anyURI ] ;
                                                            rif:slotvalue [ a rif:Var ;
                                                                    rif:varname "rule" ] ] ) ] ;
                                        rif:vars ( [ a rif:Var ;
                                                    rif:varname "outerrule" ] ) ] ] ) ] ;
            rif:then [ a rif:Do ;
                    rif:actionVar ( [ a rif:Var ;
                                rif:varname "actionlist" ] [ a rif:New ] ),
                        ( [ a rif:Var ;
                                rif:varname "defrule" ] [ a rif:New ] ) ;
                    rif:actions ( [ a rif:Assert ;
                                rif:target [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "defrule" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://www.w3.org/1999/02/22-rdf-syntax-ns#type"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:Const ;
                                                            rif:constIRI "http://clips.script/Defrule"^^xsd:anyURI ] ] ) ] ] [ a rif:Assert ;
                                rif:target [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "defrule" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://example.com/uses-patterns-as-conditional"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:Var ;
                                                            rif:varname "rule" ] ] ) ] ] [ a rif:Assert ;
                                rif:target [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "defrule" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://example.com/out-as-conditional"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:List ;
                                                            rif:items () ] ] ) ] ] [ a rif:Assert ;
                                rif:target [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "defrule" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://example.com/uses-action-from"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:Var ;
                                                            rif:varname "formula" ] ] ) ] ] [ a rif:Assert ;
                                rif:target [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "defrule" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://example.com/bound-variables"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:List ;
                                                            rif:items () ] ] ) ] ] ) ] ] ;
    rif:vars ( [ a rif:Var ;
                rif:varname "rule" ] [ a rif:Var ;
                rif:varname "formula" ] ) .

<http://example.com/init-defrule-per-implies> a rif:Forall ;
    rif:formula [ a rif:Implies ;
            rif:if [ a rif:And ;
                    rif:formulas ( [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "group" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://www.w3.org/2007/rif#sentences"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Var ;
                                                    rif:varname "sentences" ] ] ) ] [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "rule" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://www.w3.org/1999/02/22-rdf-syntax-ns#type"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Const ;
                                                    rif:constIRI "http://www.w3.org/2007/rif#Implies"^^xsd:anyURI ] ] ) ] [ a rif:External ;
                                rif:content [ a rif:Atom ;
                                        rif:args ( [ a rif:Var ;
                                                    rif:varname "sentences" ] ) ;
                                        rif:op [ a rif:Const ;
                                                rif:constIRI "http://www.w3.org/2007/rif-builtin-predicate#is-list"^^xsd:anyURI ] ] ] [ a rif:External ;
                                rif:content [ a rif:Atom ;
                                        rif:args ( [ a rif:Var ;
                                                    rif:varname "sentences" ] [ a rif:Var ;
                                                    rif:varname "rule" ] ) ;
                                        rif:op [ a rif:Const ;
                                                rif:constIRI "http://www.w3.org/2007/rif-builtin-predicate#list-contains"^^xsd:anyURI ] ] ] [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "rule" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://www.w3.org/2007/rif#if"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Var ;
                                                    rif:varname "rifcondition" ] ] ) ] [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "rifcondition" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://example.com/as-conditional"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Var ;
                                                    rif:varname "clipsconditions" ] ] ) ] [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "rule" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://www.w3.org/2007/rif#then"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Var ;
                                                    rif:varname "rifaction" ] ] ) ] [ a rif:Frame ;
                                rif:object [ a rif:Var ;
                                        rif:varname "rifaction" ] ;
                                rif:slots ( [ a rif:Slot ;
                                            rif:slotkey [ a rif:Const ;
                                                    rif:constIRI "http://example.com/as-action"^^xsd:anyURI ] ;
                                            rif:slotvalue [ a rif:Var ;
                                                    rif:varname "clipsactions" ] ] ) ] ) ] ;
            rif:then [ a rif:Do ;
                    rif:actionVar ( [ a rif:Var ;
                                rif:varname "actionlist" ] [ a rif:New ] ),
                        ( [ a rif:Var ;
                                rif:varname "defrule" ] [ a rif:New ] ) ;
                    rif:actions ( [ a rif:Assert ;
                                rif:target [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "rule" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://example.com/translate-implies-to-defrule"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:Var ;
                                                            rif:varname "defrule" ] ] ) ] ] [ a rif:Assert ;
                                rif:target [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "defrule" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://www.w3.org/1999/02/22-rdf-syntax-ns#type"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:Const ;
                                                            rif:constIRI "http://clips.script/Defrule"^^xsd:anyURI ] ] ) ] ] [ a rif:Assert ;
                                rif:target [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "defrule" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://clips.script/conditional-element"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:Var ;
                                                            rif:varname "clipsconditions" ] ] ) ] ] [ a rif:Assert ;
                                rif:target [ a rif:Frame ;
                                        rif:object [ a rif:Var ;
                                                rif:varname "defrule" ] ;
                                        rif:slots ( [ a rif:Slot ;
                                                    rif:slotkey [ a rif:Const ;
                                                            rif:constIRI "http://clips.script/action"^^xsd:anyURI ] ;
                                                    rif:slotvalue [ a rif:Var ;
                                                            rif:varname "clipsactions" ] ] ) ] ] ) ] ] ;
    rif:vars ( [ a rif:Var ;
                rif:varname "group" ] [ a rif:Var ;
                rif:varname "rule" ] [ a rif:Var ;
                rif:varname "formula" ] ) .

<http://example.com/init-defrules> a rif:Group ;
    rif:behavior [ rif:Priority 10 ] ;
    rif:sentences ( <http://example.com/init-defrule-per-implies> <http://example.com/init-defrule-per-forall> <http://example.com/add-action-to-defrule> <http://example.com/add-conditional-to-defrule> ) .

[] a rif:Document ;
    rif:payload <http://example.com/init-defrules> .

