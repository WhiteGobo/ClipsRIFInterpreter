                                rif:target [ a rif:Var ;
                                        rif:varname "innerrule" ] ] ) ] ] ;
    rif:vars ( [ a rif:Var ;
                rif:varname "innerrule" ] [ a rif:Var ;
                rif:varname "outerrule" ] [ a rif:Var ;
                rif:varname "action" ] ) .

